/*
	Generated by the Material Editor.
	Type 'materialeditor' at the console to launch the material editor.
*/
models/monsters/lostsoul2/lostsoul
{
	noSelfShadow	
	renderbump	-size 512 512 -aa 1   models/monsters/lostsoul2/lostsoul_local.tga models/monsters/lostsoul2/lostsoul_hi.ase
	unsmoothedTangents	
	{
		name	"Stage 1"
		if	parm7   // only when dead
		map	models/monsters/lostsoul2/lostsoul_ted_dis.tga
		blend	gl_zero, gl_one    // don't draw anything
		highquality	
		alphatest	0.05 + 0.3 * (time - parm7)
	}
	{
		name	"Stage 2"
		if	parm7   // only when dead
		map	models/monsters/lostsoul2/lostsoul_ted_dis.tga
		blend	gl_one, gl_zero
		highquality	
		alphatest	0.3 * (time - parm7)
	}
	diffusemap	models/monsters/lostsoul2/lostsoul_ted_d.tga
	bumpmap	models/monsters/lostsoul2/lostsoul_local.tga
	specularmap	models/monsters/lostsoul2/lostsoul_ted_s.tga
	{
		name	"Stage 6"
		if	parm7   // only when dead
		map	models/monsters/lostsoul2/lostsoul_ted_l.tga
		blend	add
		rgb	(1-(time-parm7))*sintable [ time * .25 ]
	}
	{
		name	"Stage 6a"
		if	parm7==0   // only when dead
		map	models/monsters/lostsoul2/lostsoul_ted_l.tga
		blend	add
		rgb	sintable [ time * .25 ]
	}
	{
		name	"Stage 7"
		map	models/monsters/lostsoul2/lostsoul_ted_l2.tga
		blend	add
	}
}



