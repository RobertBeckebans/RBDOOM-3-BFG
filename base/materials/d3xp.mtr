models/weapons/artifact/artifact_tech
{
	renderbump		-size 512 512 -aa 2 -colormap models/weapons/artifact/artifact_tech_local.tga models/weapons/artifact/work/artifact_tech_hp.lwo
	noselfshadow
	noshadows
	bumpmap			addnormals (models/weapons/artifact/artifact_tech_local, heightmap (models/weapons/artifact/artifact_tech_bmp,4 ) )
    {
		blend		diffusemap
		map			models/weapons/artifact/artifact_tech
		alphatest	.5
	}
	specularmap		models/weapons/artifact/artifact_tech_s
}

