
textures/rockd3xp/icesheet
{
	noShadows
        noselfShadow
	

	qer_editorimage textures/rockd3xp/icesheet_d.tga


	{
		Blend		diffusemap	
		map		textures/rockd3xp/icesheet_d.tga
		alphatest 	0.5

	}
	
 	bumpmap			textures/rockd3xp/icesheet_local.tga
	specularmap		textures/rockd3xp/icesheet_s.tga
}



textures/rockd3xp/sharprock_d3xp
{
	noselfShadow
	qer_editorimage		textures/rockd3xp/sharprock_dark
	stone

	bumpmap				addnormals (textures/rockd3xp/sharprock_local, heightmap (textures/rockd3xp/newdarkrock_bmp.tga, 10) ) 
	diffusemap			textures/rockd3xp/sharprock_dark
	specularmap			textures/rockd3xp/sharprock_dark_s
}


textures/rockd3xp/sharprock_bacteria_5
{

	noShadows
        noselfShadow



	{
		blend 		bumpmap
		map			addnormals (textures/rockd3xp/sharprock_local, heightmap (textures/rockd3xp/newdarkrock_bmp.tga, 10) ) 
		scale 2,1
	}
	{
		blend 		diffusemap
		map			textures/rockd3xp/sharprock_dark
		inversevertexColor
		scale 2,1
	}
	{
		blend		specularmap
		map			textures/rockd3xp/sharprock_dark_s
		inversevertexColor
		scale 2,1
	}
	{
		Blend		diffusemap	
		map		textures/hell3xp/bacteria_5.tga
		translate	time *.01, time*.05
		vertexColor
	}
	{

		blend		bumpmap	 
		map		textures/hell3xp/bacteria_3_local.tga
		translate	time *.01, time*.05
	}
	{

		blend		specularmap	 
		map		textures/hell3xp/bacteria_3_s.tga
		translate	time *.01, time*.05
		vertexColor

	}		    
		
}

