		inverseVertexColor
	}
	{

		blend		specularmap	 
		map		textures/rockd3xp/frozenrock2_s.tga
		inverseVertexColor

	}

		
}


textures/rockd3xp/frozenrock2_blend

{
	noSelfShadow
	noshadows

	qer_editorimage	textures/rockd3xp/frozenrock2_d.tga	


	{
		blend 		bumpmap
		map			textures/hell/wrinklewall_tile_local
		translate	.01*sintable[ time *.1 ],  	.02*sintable[ time *.1 ]

	}
	{
		blend 		diffusemap
		map			textures/hell/wrinklewall_tile
		translate	.01*sintable[ time *.1 ], 	.02*sintable[ time *.1 ]
		inverseVertexColor
	}

	{
		blend		specularmap
		map			textures/hell/wrinklewall_tile_s
		translate 	.01*sintable[ time *.1 ],	.02*sintable[ time *.1 ]
		inverseVertexColor
	}
	{

		blend		bumpmap	 
		map		textures/rockd3xp/frozenrock2_local.tga
		
	}	
	{
		Blend		diffusemap	
		map		textures/rockd3xp/frozenrock2_d.tga  	
		VertexColor
	}
	{

		blend		specularmap	 
		map		textures/rockd3xp/frozenrock2_s.tga
		VertexColor

	}

		
}



