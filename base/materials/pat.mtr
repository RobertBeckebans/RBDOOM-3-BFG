//
//////////////////////////////////

models/monsters/mancubus/mancubus_slime
{
	
	noselfShadow
    	noshadows
	flesh
	unsmoothedTangents
	clamp
	
	{					// burning corpse effect
		if	parm7			// only when dead
		
						// make a burned away alpha test for the normal skin
		blend	gl_zero, gl_one		// don't draw anything
		
		map models/monsters/mancubus/mancubus_dis.tga	// replace this with a monster-specific texture
		alphaTest 0.05 + 0.3 * (time - parm7)
	}
	{					// burning corpse effect
		if parm7			// only when dead

						// draw the fire burn at a negative polygonOffset, so it is behind the other stages
		privatePolygonOffset	-1	// stage-only polygon offset
        	blend add	
        	blend gl_one, gl_zero
		
		map models/monsters/mancubus/mancubus_dis.tga	// replace this with a monster-specific texture
		alphaTest 0.3 * (time - parm7)
	}
		forceOverlays
//		bumpmap			models/monsters/mancubus/mancubus_slime_local.tga
		diffusemap 		models/monsters/mancubus/mancubus_slime_d.tga
		bumpmap addnormals(models/monsters/mancubus/mancubus_slime_local.tga, heightmap(models/monsters/mancubus/mancubus_slime_h.tga, 2 ) )
		specularmap		models/monsters/mancubus/mancubus_slime_s.tga
}


