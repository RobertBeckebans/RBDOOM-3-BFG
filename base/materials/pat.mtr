textures/pat/patlavalava2
{
	qer_editorimage	textures/pat/patlava2.tga
	{
		map     textures/hell/lava2.tga
		scale	1 , 1
		translate	time * 0.005, time * -0.01
		rgb		fireballtable[ time * .05 ]
	}
	{
		blend add
		map     textures/hell/lava2.tga
		scale	1.4, 1.5
		translate	time * 0, time * -0.03
		rgb		fireballtable[ time * .04 ]

	}
	{
		blend add
		map     textures/hell/lava2.tga
		scale	.8, 1.1
		translate	time * -0.001, time * -0.02
		rgb		fireballtable[ time * .1 ]

	}
}

