textures/base_wall/bj_sopanel19a
{
	qer_editorimage	textures/base_wall/sopanel19a.tga
	{if ( parm7 == 0 )
			blend	bumpmap
			map		textures/base_wall/sopanel19_local.tga
	}
	{if ( parm7 == 0 )
		blend		diffusemap
		map			textures/base_wall/bj_sopanel19a.tga
	}
	{if ( parm7 == 0 )
		blend		specularmap
		map			textures/base_wall/sopanel19_s.tga
	}
	{if ( parm7 == 0 )
		blend		add
		map			textures/base_wall/sopanel19a_add.tga
		colored
	}
	//
	{if ( parm7 != 0 )
			blend	bumpmap
			map		textures/base_wall/sopanel19_local.tga
	}
	{if ( parm7 != 0 )
		blend		diffusemap
		map			textures/base_wall/sopanel19a2.tga
	}
	{if ( parm7 != 0 )
		blend		specularmap
		map			textures/base_wall/sopanel19a2_s.tga
	}
	{if ( parm7 != 0 )
		blend		add
		map			textures/base_wall/sopanel19a2_add.tga
		colored
	}
}

